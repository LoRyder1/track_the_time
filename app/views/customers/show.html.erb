<h1><%= @c.company %></h1>
<h5>info:</h5>

<hr>
<h6>Projects</h6>

<%= link_to 'New Project', new_customer_project_path(@c) %>
<% @p.each do |x| %>
<ul>
	<li><%= x.project_name  %></li>
</ul>
<% end %>

<hr>
<h6>Timeentries Report</h6>

<table>
	<tr>
		<th>Id</th>
		<th>Task</th>
		<th>Project</th>
		<th>Duration</th>
	</tr>
<% @tes.each do |t| %>
	<tr>
		<td><%= t.id %></td>
		<td><%= t.task.task_name %></td>
		<td><%= t.project.project_name %></td>
		<td><%= t.duration %></td>
	</tr>
<% end %>
</table>

<p>
	Download: 
	<%= link_to 'Excel', customer_path(format: 'xls') %>
</p>